curl -i -X GET localhost:3000/users/

curl -i -X GET localhost:3000/transactions/

curl -i -X PUT -H "Content-Type: application/json" -d '{ "monto": 999 } localhost:3000/user/:aguantemaxi_98@hotmail.com

